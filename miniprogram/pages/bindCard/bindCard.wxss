.tab{
  padding: 32rpx 12px;
  background: #e3e3e3;
  font-size: 30rpx;
  color: #666;
}
.user{
  padding: 0 20rpx;
}
.user-item{
  border-bottom:1px solid #d2cfcf;
  padding: 20rpx 10rpx;
  font-size:35rpx;
}
.label{
  width: 25%;
  text-align: right;
}
.row{
  width: 100%;
  font-size: 35rpx;
  color: #000;
  position: relative;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-align-items: center;
  align-items: center;
}
.btn{
  width: 100%;
  text-align: center;
  background: #e4393c;
  color:#fff;
  /* padding: 30rpx 0; */
  font-size: 35rpx;
  border-radius: 0;
  margin-top: 20px;
}